<!-- OUR ROOMS-->
<section class="section bg-blue p-t-120 p-b-165">
    <div class="bg-cloud"></div>
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="section-heading section-heading-2">
                    <h2 class="section-heading__title">our Rooms & Classes</h2>
                    <div class="section-heading__line">
                        <img src="~/syte/images/icon/line-white.png" alt="Line 2">
                    </div>
                </div>
            </div>
        </div>
        <div class="row no-gutters">
            @foreach (var item in Model.classroms)
            {
                <div class="col-lg-6">
                    <div class="p-r-25 p-md-r-0">
                        <div class="media media-our-class">
                            <div class="media__body">
                                <h3 class="media__title" style="font-size:23px !important">
                                    <a  asp-controller="SingleClas" asp-action="Index" asp-route-id="@item.Id">@item.Name</a>
                                </h3>
                                <span class="date-time">@item.Openhourse.ToString("HH mm") am - @item.Closehours.ToString("hh mm") pm</span>
                                <span class="date-time">@item.Startime.ToString("MMMMM d, yyyy").ToUpperInvariant()</span>
                                <div class="media-our-class__info">
                                    <div class="row no-gutters">
                                        <div class="col-6">
                                            <div class="item has-seprator">
                                                <span class="name">age</span>
                                                <span class="value">@item.Agee.Miniage-@item.Agee.Maxage years</span>
                                            </div>
                                        </div>
                                        <div class="col-6">
                                            <div class="item">
                                                <span class="name">size</span>
                                                <span class="value">@item.Size seats</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="media__img">
                                <a asp-controller="SingleClas" asp-action="Index" asp-route-id="@item.Id">
                                    <img src="~/Librarry/Class/@item.Iamgename" alt="Our room 1">
                                </a>
                                <span class="media-our-class__price">
                                    <span class="inner">$@item.Price</span>
                                </span>
                            </div>
                        </div>
                        <hr class="line-white m-l-0">
                        
                    </div>
                </div>
            }


        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="learn-more text-center m-t-60">
                    <a class="au-btn au-btn--pink" asp-controller="Clasess" asp-action="Index">
                        learn more
                        <i class="zmdi zmdi-chevron-right"></i>
                        <i class="zmdi zmdi-chevron-right"></i>
                    </a>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- END OUR ROOMS-->
